{
  "name": "soft-pink-theme",
  "displayName": "Soft Pink Theme",
  "description": "A soft pink theme for a feminine style in the code editor.",
  "version": "0.0.6",
  "main": "./out/extension.js",
  "publisher": "narimanjafari",
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Themes"
  ],
  "activationEvents": [
    "onCommand:softpink.increaseFontSize",
    "onCommand:softpink.decreaseFontSize",
    "onCommand:softpink.resetFontSize"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Soft Pink Theme",
        "uiTheme": "vs",
        "path": "./themes/softPinkColorTheme.json"
      },
      {
        "label": "Soft Pink Dark Theme",
        "uiTheme": "vs-dark",
        "path": "./themes/softPinkDarkTheme.json"
      }
    ],
    "commands": [
      {
        "command": "softpink.increaseFontSize",
        "title": "Soft Pink: Increase Font Size"
      },
      {
        "command": "softpink.decreaseFontSize",
        "title": "Soft Pink: Decrease Font Size"
      },
      {
        "command": "softpink.resetFontSize",
        "title": "Soft Pink: Reset Font Size"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "npx @vscode/vsce package"
  },
  "devDependencies": {
    "typescript": "^4.9.5",
    "@types/vscode": "^1.50.0",
    "@vscode/vsce": "^2.22.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/snowholt/pinkTheme"
  },
  "keywords": [
    "theme",
    "pink",
    "feminine",
    "soft",
    "light"
  ]
}