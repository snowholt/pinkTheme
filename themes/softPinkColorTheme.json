{
  "$schema": "vscode://schemas/color-theme",
  "name": "Soft Pink Theme",
  "type": "light",
  "semanticHighlighting": true,
  "colors": {
    "foreground": "#000000",
    "descriptionForeground": "#000000",
    "editor.background": "#FFF0F5",
    "editor.foreground": "#000000",
    "activityBar.background": "#F8BBD0",
    "activityBar.foreground": "#000000",
    "sideBar.background": "#FFEBEE",
    "sideBar.foreground": "#000000",
    "sideBarTitle.foreground": "#000000",
    "statusBar.background": "#F48FB1",
    "statusBar.foreground": "#000000",
    "editor.lineHighlightBackground": "#FCE4EC",
    "editorCursor.foreground": "#000000",
    "editorWhitespace.foreground": "#EF9A9A",
    "editorIndentGuide.background1": "#EF9A9A",
    "editorIndentGuide.activeBackground1": "#C2185B",
    "editor.selectionBackground": "#F48FB180",
    "list.activeSelectionBackground": "#F8BBD0",
    "list.activeSelectionForeground": "#000000",
    "list.hoverBackground": "#FFCDD2",
    "list.hoverForeground": "#000000",
    "titleBar.activeBackground": "#F06292",
    "titleBar.activeForeground": "#000000",
    "tab.activeBackground": "#FFF0F5",
    "tab.activeForeground": "#000000",
    "tab.inactiveBackground": "#FFEBEE",
    "tab.inactiveForeground": "#000000",
    "tab.activeBorderTop": "#C2185B",
    "panelTitle.activeForeground": "#000000",
    "panelTitle.inactiveForeground": "#000000",
    "panel.background": "#FFF0F5",
    "input.foreground": "#000000",
    "input.background": "#FFF0F5",
    "input.placeholderForeground": "#00000080",
    "chat.requestBackground": "#FFEBEE",
    "chat.requestBorder": "#F48FB1"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#000000",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#D81B60",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#880E4F"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "meta.function-call"
      ],
      "settings": {
        "foreground": "#AD1457"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string"
      ],
      "settings": {
        "foreground": "#4CAF50"
      }
    },
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#FF9800"
      }
    },
    {
      "name": "Classes",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.class"
      ],
      "settings": {
        "foreground": "#6A1B9A",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Headings",
      "scope": [
        "markup.heading",
        "entity.name.section"
      ],
      "settings": {
        "foreground": "#C2185B",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "foreground": "#880E4F",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "foreground": "#880E4F",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Code",
      "scope": [
        "markup.inline.raw",
        "markup.fenced_code.block"
      ],
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "Markdown Quote",
      "scope": [
        "markup.quote"
      ],
      "settings": {
        "foreground": "#000000",
        "fontStyle": "italic"
      }
    }
  ],
  "semanticTokenColors": {
    "variable.readonly": "#C2185B",
    "variable.defaultLibrary": "#6A1B9A",
    "function.defaultLibrary": "#AD1457",
    "class": "#6A1B9A",
    "interface": "#7B1FA2",
    "type": "#7B1FA2",
    "parameter": "#880E4F"
  }
}