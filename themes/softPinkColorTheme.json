{
  "$schema": "vscode://schemas/color-theme",
  "name": "Soft Pink Theme",
  "type": "light",
  "colors": {
    "editor.background": "#FFFFFF",
    "editor.foreground": "#000000",
    "terminal.background": "#FFFFFF",
    "terminal.foreground": "#000000",
    "terminal.ansiBlack": "#333333",
    "terminal.ansiBlue": "#0066CC",
    "terminal.ansiCyan": "#00AAAA",
    "terminal.ansiGreen": "#00AA00",
    "terminal.ansiMagenta": "#AA00AA",
    "terminal.ansiRed": "#E91E63",
    "terminal.ansiWhite": "#EEEEEE",
    "terminal.ansiYellow": "#AA5500",
    "terminal.ansiBrightBlack": "#666666",
    "terminal.ansiBrightBlue": "#0099FF",
    "terminal.ansiBrightCyan": "#00CCCC",
    "terminal.ansiBrightGreen": "#00CC00",
    "terminal.ansiBrightMagenta": "#CC00CC",
    "terminal.ansiBrightRed": "#FF4081",
    "terminal.ansiBrightWhite": "#FFFFFF",
    "terminal.ansiBrightYellow": "#CC7700",
    "activityBarBadge.background": "#FF80AB",
    "activityBar.background": "#FFEBEE",
    "activityBar.foreground": "#880E4F",
    "activityBar.activeBorder": "#C2185B",
    "activityBar.activeBackground": "#FCE4EC",
    "activityBarBadge.foreground": "#FFFFFF",
    "activityBar.inactiveForeground": "#AD1457",
    "sideBarTitle.foreground": "#F06292",
    "list.activeSelectionForeground": "#F06292",
    "list.inactiveSelectionForeground": "#F06292",
    "list.highlightForeground": "#F06292",
    "scrollbarSlider.activeBackground": "#F8BBD080",
    "editorSuggestWidget.highlightForeground": "#F06292",
    "textLink.foreground": "#F06292",
    "progressBar.background": "#F06292",
    "pickerGroup.foreground": "#F06292",
    "tab.activeBorder": "#F06292",
    "notificationLink.foreground": "#F06292",
    "editorWidget.resizeBorder": "#F06292",
    "editorWidget.border": "#F06292",
    "settings.modifiedItemIndicator": "#F06292",
    "settings.headerForeground": "#F06292",
    "panelTitle.activeBorder": "#F06292",
    "breadcrumb.activeSelectionForeground": "#F06292",
    "menu.selectionForeground": "#F06292",
    "menubar.selectionForeground": "#F06292",
    "editor.selectionBackground": "#F8BBD080",
    "statusBarItem.hoverBackground": "#FFCDD2",
    "statusBar.background": "#FFEBEE",
    "statusBar.foreground": "#880E4F",
    "titleBar.activeBackground": "#FFEBEE",
    "titleBar.activeForeground": "#880E4F",
    "sideBar.background": "#FFF9FB",
    "button.background": "#F06292",
    "button.foreground": "#FFFFFF",
    "diffEditor.insertedTextBackground": "#F8BBD050",
    "diffEditor.insertedTextBorder": "#EC407A33",
    "diffEditor.removedTextBackground": "#FFCDD250",
    "diffEditor.removedTextBorder": "#E9183633",
    "editorGutter.addedBackground": "#EC407A",
    "editorGutter.deletedBackground": "#E91836",
    "editorGutter.modifiedBackground": "#9C27B0",
    "gitDecoration.addedResourceForeground": "#C2185B",
    "gitDecoration.modifiedResourceForeground": "#9C27B0",
    "gitDecoration.deletedResourceForeground": "#E91836",
    "gitDecoration.untrackedResourceForeground": "#7B1FA2",
    "gitDecoration.ignoredResourceForeground": "#BDBDBD",
    "gitDecoration.conflictingResourceForeground": "#FF5252",
    "editorInlayHint.background": "#FFF0F5",
    "editorInlayHint.foreground": "#AD1457",
    "icon.foreground": "#880E4F",
    
    // List colors
    "list.errorForeground": "#C2185B",
    "list.warningForeground": "#BB6BD9",
    "editorBracketMatch.background": "#F8BBD080",
    "editorBracketMatch.border": "#F06292",
    
    // Markdown styling - using standard properties
    "textBlockQuote.background": "#FFEBEE",
    "textBlockQuote.border": "#F8BBD0",
    "textCodeBlock.background": "#E3F2FD",
    "textLink.activeForeground": "#C2185B",
    
    // Only include supported chat properties
    "chat.slashCommandBackground": "#FCE4EC",
    "chat.slashCommandForeground": "#000000"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#AD1457AA"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#EC407A"
      }
    },
    {
      "name": "Keywords",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#C2185B"
      }
    },
    {
      "name": "Functions",
      "scope": ["entity.name.function", "meta.function-call"],
      "settings": {
        "foreground": "#D81B60"
      }
    },
    {
      "name": "Strings",
      "scope": ["string", "constant.other.symbol"],
      "settings": {
        "foreground": "#F06292"
      }
    },
    {
      "name": "Classes",
      "scope": ["entity.name.type", "entity.name.class"],
      "settings": {
        "foreground": "#880E4F"
      }
    },
    {
      "name": "Markdown - Code",
      "scope": [
        "markup.fenced_code.block.markdown",
        "markup.inline.raw.markdown"
      ],
      "settings": {
        "foreground": "#0033CC",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown - Lists",
      "scope": [
        "markup.list.numbered.markdown",
        "markup.list.unnumbered.markdown",
        "punctuation.definition.list.begin.markdown"
      ],
      "settings": {
        "foreground": "#6A1B9A",
        "fontStyle": "bold"
      }
    }
  ],
  "editor.fontFamily": "'Fira Code', 'Source Code Pro', 'Comic Sans MS', monospace",
  "editor.fontSize": 14
}
